<div class="row app-row">

  <% for app in @apps: %>
    <div class="col-md-4">
      <div data-path="<%= app.path %>" class="app-icon app icon-small <%= app.iconColor %>">
        <div class="gloss"></div>
        <div class="text"><%= app.iconLabel %></div>
      </div>
      <div class="labels">Pedidos</div>
    </div>
  
  <% end %>

</div>
  
  
